        $(document).ready(function () {
            var roid = document.getElementById("permissionRole").value;
            var listPer = roid.split(',');

            var adminCauHinh = listPer.indexOf('@RoleContext.Administration');
            var adCH_SanLuong = listPer.indexOf('@RoleContext.Administration_UpdateFile');
            var adCH_CongThuc = listPer.indexOf('@RoleContext.Administration_UpdateRecipe');
            var adminQuanTri = listPer.indexOf('@RoleContext.Expertise');
            var adQT_VaiTro = listPer.indexOf('@RoleContext.Expertise_Roles');
            var adQT_NguoiDung = listPer.indexOf('@RoleContext.Expertise_Accounts');

            if (adminCauHinh == -1 && adCH_SanLuong == -1 && adCH_CongThuc == -1) {
                $('#adminCauHinh').css('display', 'none');
            } else {
                if (adCH_SanLuong == -1) {
                    $('#adCH_SanLuong').css('display', 'none');
                }
                if (adCH_CongThuc == -1) {
                    $('#adCH_CongThuc').css('display', 'none');
                }
            }
            if (adminQuanTri == -1 && adQT_VaiTro == -1 && adQT_NguoiDung == -1) {
                $('#adminQuanTri').css('display', 'none');
            } else {
                if (adQT_VaiTro == -1) {
                    $('#adQT_VaiTro').css('display', 'none');
                }
                if (adQT_NguoiDung == -1) {
                    $('#adQT_NguoiDung').css('display', 'none');
                }
            }
        });

        $.ajax({
                url: "@Url.Action("layoutUser","Home")",
                type: 'GET',
            success: function (data) {
                $("#layoutUser").html(data);
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    showMessage("Lỗi thay đổi mật khẩu", false)
                }
            });
